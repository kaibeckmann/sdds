<configuration>
  <list name="Board">
    <value name="Raven" value="BOARD_RAVEN"/>
  </list>
   
    <!--Frequency configuration-->
  <list name="Frequency">
    <if option="Board" value="Raven" default="4MHz">
      <value name="4MHz" value="HAL_4MHz"/>
      <value name="8MHz" value="HAL_8MHz"/>
    </if>
  </list>
    
  <!--Application interaface configuration-->
    <list name="Application interface">
    <if option="Board" value="Raven" default="USART">
      <value name="USART" value="APP_INTERFACE_USART"/>
    </if>		
  </list>
  <!--Interaface channel configuration-->
  <list name="Interface channel" define="APP_USART_CHANNEL">
    <if option="Board" value="Raven" default="Channel 1">
      <value name="Channel 1" value="USART_CHANNEL_1"/>
    </if>
  </list>
</configuration>
