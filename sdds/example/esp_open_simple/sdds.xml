<project
    name = "mywrite"
    script = "sdds.gsl"
    endian = "little"
    os = "freertos"
	ip =  "192.168.178.21"
    >
    <!--
        Set interface, ip and port for this project
    iface = ""
    ip = ""
    port = ""
    >
    -->
    ""

    <!--
        Include your topic files here. You can include topics from multiple files.
        Only those topics will be generated that have at least one matching role.
    -->
