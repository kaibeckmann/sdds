[PowerMeter]

/* Anzeige der derzeitigen Leistung eines elektrischen Verbrauchers in Watt. Der Verbrauch wird regelmäßig 1x pro Sekunde veröffentlicht */

domain: 0
topic: 14

/* da die generierung keine typedefs kann, datentyp direkt angegeben */
DDS_unsigned_short id 
DDS_unsigned_short watt

[PowerMeterAverage]

/* Anzeige de Leistungsverbrauchs von Geräte über verschiedene Zeitintervalle. */

domain: 0
topic: 15
/* da die generierung keine typedefs kann, datentyp direkt angegeben */
DDS_unsigned_short id 
/* Durchschnittsverbrauch in der letzten Minute */
DDS_unsigned_short watt_minute 
/* Durchschnittsverbrauch in der letzten Stunde */
DDS_unsigned_short watt_hour
/* Durchschnittsverbrauch seit einem Tag */ 
DDS_unsigned_short watt_day 
/* Durchschnittsverbrauch seit einer Woche */
DDS_unsigned_short watt_week 
/* Durchschnittsverbrauch seit einem Monat */
DDS_unsigned_short watt_month
/* Durchschnittsverbrauch seit einem Jahr */ 
DDS_unsigned_short watt_year 

