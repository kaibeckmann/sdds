<project
    name = "messung3_contiki"
    script = "sdds.gsl"
    endian = "little"
    os = "contiki"
    >
    Messung 3 auf Contiki

    <!--
         iface: this node shall use
         ip: ipv6 of the iface
         port: to send to
     -->
    <contiki jackdaw = "fe80::12:13ff:fe14:1516" board = "fe80::00ff:fe00:0001" />

    <!--
        Includes are processed first, so XML in included files will be
        part of the XML tree
    -->
    <include filename = "../topics/messung.xml" />

    <role topic = "alpha" type = "subscriber" />

    <role topic = "beta" type = "publisher">
        <subscriber ip = "fe80::12:13ff:fe14:1516" />
    </role>
</project>
