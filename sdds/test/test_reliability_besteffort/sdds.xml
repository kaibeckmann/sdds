<project
    name = "test_reliability_besteffort"
    script = "sdds.gsl"
    endian = "little"
    os = "linux"
    iface = "tap0"
    ip = "fe80::c0ef:62ff:fed4:d26a"
    protocol = "AF_INET6"
    port = "23234"
    >
    Linux basic publisher example

    <!--
        Includes are processed first, so XML in included files will be
        part of the XML tree
    -->

    <define name = "SDDS_HAS_QOS_RELIABILITY" />

    <define name = "SDDS_NET_MAX_BUF_SIZE" value = "256"/>
    <define name = "SDDS_NET_MAX_LOCATOR_COUNT" value = "25"/>
    <define name = "SDDS_QOS_RELIABILITY_MAX_TOPIC_PARTICIPANTS" value = "1"/>

    <include filename = "../topics/basic.xml" />
    <include filename = "../topics/qos_reliability.xml" />

    <role topic = "testQosReliabilityBasicBesteffort" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityBasicBesteffort" type = "subscriber"/>

    <role topic = "testQosReliabilitySmallBesteffort" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilitySmallBesteffort" type = "subscriber"/>

    <role topic = "testQosReliabilityBigBesteffort" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityBigBesteffort" type = "subscriber"/>

    <role topic = "testQosReliabilityHugeBesteffort" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityHugeBesteffort" type = "subscriber"/>

</project>
