<project
    name = "test_ucast_reliability_reliable_ack"
    script = "sdds.gsl"
    endian = "little"
    os = "linux"
    iface = "tap0"
    ip = "fe80::c0ef:62ff:fed4:d26a"
    protocol = "AF_INET6"
    port = "23234"
    >
    Linux basic publisher example

    <!--
        Includes are processed first, so XML in included files will be
        part of the XML tree
    -->
    <define name = "FEATURE_SDDS_MULTICAST_ENABLED" />
    <define name = "FEATURE_SDDS_BUILTIN_TOPICS_ENABLED" />
    <define name = "FEATURE_SDDS_DISCOVERY_ENABLED" />


    <define name = "SDDS_HAS_QOS_RELIABILITY" />

    <define name = "SDDS_NET_MAX_BUF_SIZE" value = "256"/>
    <define name = "SDDS_NET_MAX_LOCATOR_COUNT" value = "25"/>
    <define name = "SDDS_QOS_HISTORY_DEPTH" value = "5"/>
    <define name = "SDDS_QOS_RELIABILITY_MAX_TOPIC_PARTICIPANTS" value = "1"/>
    <define name = "SDDS_QOS_RELIABILITY_RELIABLE_SAMPLES_SIZE" value = "4"/>

    <define name = "SDDS_NET_MAX_OUT_QUEUE" value = "3"/>


    <include filename = "../topics/basic.xml" />
    <include filename = "../topics/qos_reliability.xml" />


    <role topic = "strings" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityBasicReliableAck" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilitySmallReliableAck" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityBigReliableAck" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>
    <role topic = "testQosReliabilityHugeReliableAck" type = "publisher">
        <subscriber ip = "fe80::c0ef:62ff:fed4:d26a" port = "23234"/>
    </role>


    <role topic = "strings" type = "subscriber"/>
    <role topic = "testQosReliabilityBasicReliableAck" type = "subscriber"/>
    <role topic = "testQosReliabilitySmallReliableAck" type = "subscriber"/>
    <role topic = "testQosReliabilityBigReliableAck" type = "subscriber"/>
    <role topic = "testQosReliabilityHugeReliableAck" type = "subscriber"/>


</project>
